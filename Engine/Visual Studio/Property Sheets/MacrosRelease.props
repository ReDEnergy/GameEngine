<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <EngineConfiguration>Release</EngineConfiguration>
    <GLConfiguration>Release</GLConfiguration>
    <LibraryConfiguration>Release</LibraryConfiguration>
  </PropertyGroup>
  <PropertyGroup>
    <IncludePath>$(IncludePath)</IncludePath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>NDEBUG;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="EngineConfiguration">
      <Value>$(EngineConfiguration)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="GLConfiguration">
      <Value>$(GLConfiguration)</Value>
    </BuildMacro>
    <BuildMacro Include="LibraryConfiguration">
      <Value>$(LibraryConfiguration)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>